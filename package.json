{
  "name": "@brandextract/tinymce-plugin-video",
  "version": "0.1.0",
  "description": "A TinyMCE plugin to embed a video",
  "main": "plugin.js",
  "scripts": {
    "build": "node_modules/.bin/uglifyjs --compress properties,dead_code --mangle --source-map --output plugin.min.js plugin.js",
    "docs": "cp -r plugin.min.js.map plugin.min.js img docs/scripts/tinymce/plugins/video",
    "git": "git add -u",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "build",
    "docs",
    "git"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrandExtract/tinymce-plugin-video.git"
  },
  "keywords": [
    "TinyMCE",
    "plugin",
    "video",
    "YouTube",
    "Vimeo"
  ],
  "author": "BrandExtract",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrandExtract/tinymce-plugin-video/issues"
  },
  "homepage": "https://github.com/BrandExtract/tinymce-plugin-video#readme",
  "devDependencies": {
    "pre-commit": "^1.2.2",
    "uglify-js": "^3.17.4"
  }
}
